#suite from_decimal_to_int_test

#test from_decimal_to_int_1
  //                            553402.32229718589441
  //   -dec->int---------------------------------------
  //                            553402

  es_decimal one = {{1, 2, 3, 917504}};
  int res = 0;
  ck_assert_int_eq(es_from_decimal_to_int(one, &res), 0);
  ck_assert_int_eq(res, 553402);

