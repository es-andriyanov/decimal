#suite from_decimal_to_float_test

#test from_decimal_to_float_1
  //                            553402.32229718589441
  //   -dec->float-------------------------------------
  //  553402.3

  es_decimal one = {{1, 2, 3, 917504}};
  float res = 0;
  ck_assert_int_eq(es_from_decimal_to_float(one, &res), 0);
  ck_assert_float_eq(res, 553402.3);

