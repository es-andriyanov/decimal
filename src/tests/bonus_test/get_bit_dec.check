#suite get_bit_dec_test

#test get_bit_dec_1
  es_decimal test = {{(int)0b00000000000000000000000000000011, (int)0b00000000000000000000000000000001, (int)0b00000000000000000000000000000001, (int)0b00000000000000000000000000000001}};
  ck_assert_int_eq(get_bit_dec(test, 1), 1);

#test get_bit_dec_2
  es_decimal test = {{(int)0b00000000000000000000000000000011, (int)0b00000000000000000000000000000001, (int)0b00000000000000000000000000000001, (int)0b00000000000000000000000000000001}};
  ck_assert_int_eq(get_bit_dec(test, 0), 1);

#test get_bit_dec_3
  es_decimal test = {{(int)0b00000000000000000000000000000011, (int)0b00000000000000000000000000000001, (int)0b00000000000000000000000000000001, (int)0b00000000000000000000000000000001}};
  ck_assert_int_eq(get_bit_dec(test, 2), 0);

#test get_bit_dec_4
  es_decimal test = {{(int)0b00000000000000000000000000000011, (int)0b00000000000000000000000000000001, (int)0b00000000000000000000000000000001, (int)0b00000000000000000000000000000001}};
  ck_assert_int_eq(get_bit_dec(test, 31), 0);

#test get_bit_dec_5
  es_decimal test = {{(int)0b00000000000000000000000000000011, (int)0b00000000000000000000000000000001, (int)0b00000000000000000000000000000001, (int)0b00000000000000000000000000000001}};
  ck_assert_int_eq(get_bit_dec(test, 32), 1);

#test get_bit_dec_6
  es_decimal test = {{(int)0b00000000000000000000000000000011, (int)0b00000000000000000000000000000001, (int)0b00000000000000000000000000000001, (int)0b00000000000000000000000000000001}};
  ck_assert_int_eq(get_bit_dec(test, -1), -1);